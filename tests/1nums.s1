(d l20   ("" "Один" "Два" "Три" "Четыре" "Пять"
          "Шесть" "Семь" "Восемь" "Девять" "Десять"
          "Одинадцать" "Двенадцать" "Тринадцать"
          "Четырнадцать" "Пятнадцать" "Шеснадцать"
          "Семьнадцать" "Восемьнадцать" "Девятнадцать"))

(d l100  ("Двадцать" "Тридцать" "Сорок" "Пятьдесят"
          "Шестьдесят" "Семьдесят" "Восемдесят" "Девяносто"))

(d l1000 ("Сто" "Двести" "Тристо" "Четыресто" "Пятсот"
          "Шестьсот" "Семьсот" "Восемьсот" "Девятсот"))

(d pg100
 (l (num)
  (if (less num 20)
    (print (lref l20 num))
  (print
   (lref l100 (sub (divi num 10) 2))
   " " (lref l20 (mod num 10))))))

(d pg1000
 (l (num)
  (if (less num 100) ()
   (print (lref l1000
           (sub (divi num 100) 1)) " "))
  (pg100 (mod num 100))))

(d timer
 (l (sec)
  (d cont (l ()
    (pg1000 sec)
    (eep 1)
    (print NL)
    (timer (sub sec 1))))

  (if (ieq sec 0) ()
   (cont))))

(timer 999)
